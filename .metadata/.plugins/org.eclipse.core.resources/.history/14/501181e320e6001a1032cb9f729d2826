    <elementParameter field="MEMO_SQL" name="QUERY" value="&quot;CREATE TABLE #ReporteGlobalMallas &#xD;&#xA;(Id INT IDENTITY(1, 1) PRIMARY KEY,&#xD;&#xA;Fila BIGINT,&#xD;&#xA;[Id Malla] INT,&#xD;&#xA;[Malla] VARCHAR(255),&#xD;&#xA;[Fecha Creación] VARCHAR(255),&#xD;&#xA;[Estado] VARCHAR(255),&#xD;&#xA;IdNominaMalla INT,&#xD;&#xA;Escuela_Id INT,&#xD;&#xA;[Escuela] VARCHAR(255),&#xD;&#xA;[Versión Malla] INT,&#xD;&#xA;[Vigencia Versión] VARCHAR(255),&#xD;&#xA;[Version_VigenciaDesde] VARCHAR(255),&#xD;&#xA;Version_VigenciaHasta VARCHAR(255),&#xD;&#xA;[Estado Versión] VARCHAR(255), &#xD;&#xA;[Número Itinerario] INT,&#xD;&#xA;Itinerario_VigenciaDesde VARCHAR(255),&#xD;&#xA;Itinerario_VigenciaHasta VARCHAR(255),&#xD;&#xA;[Nombre Itinerario] VARCHAR(255),&#xD;&#xA;[Estado Itinerario] VARCHAR(255),&#xD;&#xA;[Vigencia Itinerario] VARCHAR(255),&#xD;&#xA;Persona_Id INT,&#xD;&#xA;[RUT] VARCHAR(255),&#xD;&#xA;[Nombre Colaborador] VARCHAR(255),&#xD;&#xA;EstadoColaborador VARCHAR(255), &#xD;&#xA;[Email] VARCHAR(255), &#xD;&#xA;SociedadContratante_Id INT,&#xD;&#xA;[Sociedad Contratante] VARCHAR(255),  &#xD;&#xA;UOrganizacional_Id INT, &#xD;&#xA;[Unidad Organizacional] VARCHAR(255),&#xD;&#xA;UNegocio_Id INT,&#xD;&#xA;[Unidad de Negocio] VARCHAR(255), &#xD;&#xA;Area_Id INT, &#xD;&#xA;[Área]  VARCHAR(255),&#xD;&#xA;Local_Id INT, &#xD;&#xA;Local_Codigo  VARCHAR(255), &#xD;&#xA;[Local] VARCHAR(255),&#xD;&#xA;Cargo_Id INT,&#xD;&#xA;[Cargo] VARCHAR(255),&#xD;&#xA;[Familia Cargo] VARCHAR(255),&#xD;&#xA;[Rut Jefe] VARCHAR(255),&#xD;&#xA;[Nombre Jefe] VARCHAR(255), &#xD;&#xA;[Email Jefe] VARCHAR(255), &#xD;&#xA;Avance FLOAT,&#xD;&#xA;[Cantidad] INT, &#xD;&#xA;Aprobadas INT,&#xD;&#xA;Reprobadas INT,&#xD;&#xA;Pendientes INT,&#xD;&#xA;Procesado INT,&#xD;&#xA;NominaActiva BIT&#xD;&#xA;)&#xD;&#xA;&#xD;&#xA;INSERT INTO  #ReporteGlobalMallas&#xD;&#xA;(Fila,[Id Malla],[Malla],[Fecha Creación],[Estado],IdNominaMalla,Escuela_Id,[Escuela],[Versión Malla],[Vigencia Versión],[Version_VigenciaDesde],Version_VigenciaHasta,&#xD;&#xA;[Estado Versión], [Número Itinerario],Itinerario_VigenciaDesde,Itinerario_VigenciaHasta,[Nombre Itinerario],[Estado Itinerario],[Vigencia Itinerario],Persona_Id,&#xD;&#xA;[RUT],[Nombre Colaborador], EstadoColaborador, [Email], SociedadContratante_Id, [Sociedad Contratante],  UOrganizacional_Id, [Unidad Organizacional],UNegocio_Id,&#xD;&#xA;[Unidad de Negocio], Area_Id, [Área],Local_Id, Local_Codigo, [Local],Cargo_Id,[Cargo],[Familia Cargo],[Rut Jefe],[Nombre Jefe], [Email Jefe], Avance,[Cantidad], Aprobadas,&#xD;&#xA;Reprobadas,Pendientes,Procesado,NominaActiva)&#xD;&#xA;&#xD;&#xA;SELECT  &#xD;&#xA;&#x9;&#x9;ROW_NUMBER() OVER (ORDER BY m.Id DESC, p.ApellidoPaterno ASC),&#xD;&#xA;&#x9;&#x9;m.Id,&#xD;&#xA;&#x9;&#x9;m.Nombre,&#xD;&#xA;&#x9;&#x9;CONVERT(VARCHAR(10), ISNULL(m.FechaCreacion, ''), 103), &#xD;&#xA;&#x9;&#x9;(CASE WHEN m.Activo = 1 THEN 'Activa' ELSE 'No Activa' END),&#xD;&#xA;&#x9;&#x9;nm.Id,&#xD;&#xA;&#x9;&#x9;esc.Id,&#xD;&#xA;&#x9;&#x9;esc.Nombre,&#xD;&#xA;&#x9;&#x9;vm.[Version],&#xD;&#xA;&#x9;&#x9;ISNULL(CONVERT(VARCHAR(10), ISNULL(vm.FechaInicio, ''), 103), '') + ' - ' + ISNULL(CONVERT(VARCHAR(10), vm.FechaTermino, 103), ''),&#xD;&#xA;&#x9;&#x9;vm.FechaInicio,&#xD;&#xA;&#x9;&#x9;vm.FechaTermino, &#xD;&#xA;&#x9;&#x9;(CASE WHEN vm.Activo = 1 THEN 'Vigente' ELSE 'No Vigente' END),&#xD;&#xA;&#x9;&#x9;mi.Id,&#xD;&#xA;&#x9;&#x9;mi.FechaInicio, &#xD;&#xA;&#x9;&#x9;mi.FechaTermino,&#xD;&#xA;&#x9;&#x9;mi.Nombre, &#xD;&#xA;&#x9;&#x9;(CASE WHEN mi.Activo = 1 THEN 'Vigente' ELSE 'No Vigente' END), &#xD;&#xA;&#x9;&#x9;ISNULL(CONVERT(VARCHAR(10), ISNULL(mi.FechaInicio, ''), 103), '') + ' - ' + ISNULL(CONVERT(VARCHAR(10), mi.FechaTermino, 103), ''),&#x9;&#x9;&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;p.Id,&#xD;&#xA;&#x9;&#x9;p.IdentificacionUnica + '-' + p.Dv, &#xD;&#xA;&#x9;&#x9;p.ApellidoPaterno + ' ' + p.ApellidoMaterno + ' ' + p.Nombre, &#xD;&#xA;&#x9;&#x9;(CASE WHEN p.Activo = 1 THEN 'Vigente' ELSE 'No Vigente' END),&#xD;&#xA;&#x9;&#x9;ISNULL(p.Email, ''),&#xD;&#xA;&#x9;&#x9;pl.IdSociedadContratante, &#xD;&#xA;&#x9;&#x9;sc.Nombre,&#xD;&#xA;&#x9;&#x9;uo.Id,&#xD;&#xA;&#x9;&#x9;uo.Nombre, &#x9;&#x9;&#xD;&#xA;&#x9;&#x9;un.Id,&#xD;&#xA;&#x9;&#x9;un.Nombre, &#xD;&#xA;&#x9;&#x9;a.Id,&#xD;&#xA;&#x9;&#x9;a.Nombre, &#xD;&#xA;&#x9;&#x9;loc.Id, &#xD;&#xA;&#x9;&#x9;loc.CodigoLocal, &#xD;&#xA;&#x9;&#x9;loc.NombreLocal,&#xD;&#xA;&#x9;&#x9;c.Id, &#xD;&#xA;&#x9;&#x9;c.Nombre, &#x9;&#x9;&#xD;&#xA;&#x9;&#x9;fc.Nombre,&#xD;&#xA;&#x9;&#x9;pj.IdentificacionUnica + '-' + pj.Dv, &#xD;&#xA;&#x9;&#x9;pj.Nombre + ' ' + pj.ApellidoPaterno + ' ' + pj.ApellidoMaterno, &#xD;&#xA;&#x9;&#x9;pj.Email,&#x9;&#x9;&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;CAST(CAST(CASE WHEN (SELECT COUNT(*) FROM MA_Componente mc&#xD;&#xA;&#x9;&#x9;JOIN MA_Seccion ms ON ms.Id = mc.IdSeccion AND ms.IdSociedad = mc.IdSociedad WHERE ms.IdVersion = vm.Id) = 0 THEN CAST(0 AS FLOAT) &#xD;&#xA;&#x9;&#x9;ELSE CAST(nm.CantUCAprobadas AS FLOAT)/CAST((SELECT COUNT(*) FROM MA_Componente mc JOIN MA_Seccion ms ON ms.Id = mc.IdSeccion AND ms.IdSociedad = mc.IdSociedad&#xD;&#xA;&#x9;&#x9;WHERE ms.IdVersion = vm.Id) AS FLOAT)*100.0 END AS DECIMAL(18,2)) AS FLOAT),&#x9;&#xD;&#xA;&#x9;&#x9;(SELECT COUNT(*) FROM MA_Componente mc JOIN MA_Seccion ms ON ms.Id = mc.IdSeccion AND ms.IdSociedad = mc.IdSociedad WHERE ms.IdVersion = vm.Id), &#xD;&#xA;&#x9;&#x9;nm.CantUCAprobadas,&#x9;&#xD;&#xA;&#x9;&#x9;nm.CantUCReprobadas, &#xD;&#xA;&#x9;   (SELECT COUNT(*) FROM MA_Componente mc JOIN MA_Seccion ms ON ms.Id = mc.IdSeccion AND ms.IdSociedad = mc.IdSociedad WHERE ms.IdVersion = vm.Id) - (nm.CantUCAprobadas + nm.CantUCReprobadas),&#xD;&#xA;&#x9;&#x9;0,&#xD;&#xA;&#x9;&#x9;nm.Activo&#x9;&#xD;&#xA;&#x9;&#x9;FROM MA_Malla m&#xD;&#xA;&#x9;LEFT JOIN MA_Version vm&#xD;&#xA;&#x9;&#x9;ON m.Id = vm.IdMalla&#xD;&#xA;&#x9;&#x9;&#x9;AND m.IdSociedad = vm.IdSociedad&#xD;&#xA;&#x9;JOIN MA_Itinerario mi&#xD;&#xA;&#x9;&#x9;ON vm.Id = mi.IdVersion&#xD;&#xA;&#x9;&#x9;&#x9;AND vm.IdSociedad = mi.IdSociedad&#xD;&#xA;&#x9;JOIN MA_Nomina nm&#xD;&#xA;&#x9;&#x9;ON mi.Id = nm.IdItinerario&#xD;&#xA;&#x9;&#x9;&#x9;AND mi.IdSociedad = nm.IdSociedad&#xD;&#xA;&#x9;JOIN Escuela esc&#xD;&#xA;&#x9;&#x9;ON esc.Id = m.IdEscuela&#xD;&#xA;&#x9;LEFT JOIN Personas p&#xD;&#xA;&#x9;&#x9;ON p.Id = nm.IdPersona&#xD;&#xA;&#x9;JOIN PosicionLaboral pl&#xD;&#xA;&#x9;&#x9;ON pl.IdPersona = p.Id&#xD;&#xA;&#x9;&#x9;&#x9;AND pl.Activo = 1 &#xD;&#xA;&#x9;&#x9;&#x9;AND pl.Estado = 2&#xD;&#xA;&#x9;JOIN InformacionPersonal ip&#xD;&#xA;&#x9;&#x9;ON ip.IdPersona = p.Id&#xD;&#xA;&#x9;JOIN UnidadesOrganizacional uo&#xD;&#xA;&#x9;&#x9;ON pl.IdUnidadOrganizacional = uo.Id&#xD;&#xA;&#x9;LEFT JOIN Cargos c&#xD;&#xA;&#x9;&#x9;ON c.Id = pl.IdCargo&#xD;&#xA;&#x9;LEFT JOIN UnidadesNegocio un&#xD;&#xA;&#x9;&#x9;ON un.Id = pl.IdUnidadNegocio&#xD;&#xA;&#x9;JOIN Sociedad sc&#xD;&#xA;&#x9;&#x9;ON sc.Id = pl.IdSociedadContratante&#xD;&#xA;&#x9;&#x9;&#x9;AND sc.Activo = 1&#xD;&#xA;&#x9;LEFT JOIN Area a&#xD;&#xA;&#x9;&#x9;ON a.Id = ip.IdArea&#xD;&#xA;&#x9;LEFT JOIN [Locales] loc&#xD;&#xA;&#x9;&#x9;ON loc.Id = ip.IdLocal&#xD;&#xA;&#x9;LEFT JOIN FamiliaCargo fc&#xD;&#xA;&#x9;&#x9;ON c.IdFamiliaCargo = fc.Id&#xD;&#xA;&#x9;LEFT JOIN Personas pj&#xD;&#xA;&#x9;&#x9;ON pl.IdPersonaJefe = pj.Id&#xD;&#xA;&#x9;&#x9;&#x9;AND pj.Activo = 1&#xD;&#xA;&#x9;WHERE pl.Id = (SELECT MAX(pl1.Id) FROM PosicionLaboral pl1&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;WHERE pl1.IdPersona = pl.IdPersona&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;AND pl1.Estado = pl.Estado)&#xD;&#xA;&#x9;AND pl.Estado = 2&#xD;&#xA;&#x9;AND nm.Activo = 1&#xD;&#xA;&#x9;AND m.IdSociedad = &quot;+ context.IdSociedad +&quot;&#xD;&#xA;&#x9;ORDER BY &#xD;&#xA;&#x9;m.Id DESC,&#xD;&#xA;&#x9;p.ApellidoPaterno ASC&#xD;&#xA;&#xD;&#xA;CREATE TABLE #UC_ReporteGlobal &#xD;&#xA;&#x9;(Id INT IDENTITY(1, 1) PRIMARY KEY,IdReporteGlobal INT,IdUC INT,Fecha VARCHAR(255),Nota VARCHAR(255), Estado VARCHAR(255));&#xD;&#xA;WITH cteUninadesCurriculares (idUc) AS &#xD;&#xA;&#x9;(&#xD;&#xA;&#x9;SELECT distinct  mc.IdUnidadCurricular&#xD;&#xA;&#x9;&#x9;FROM #ReporteGlobalMallas&#xD;&#xA;&#x9;&#x9;&#x9;LEFT JOIN MA_Version vm ON vm.IdMalla = #ReporteGlobalMallas.[Id Malla]&#xD;&#xA;&#x9;&#x9;&#x9;LEFT JOIN MA_Seccion ms&#x9;ON vm.Id = ms.IdVersion AND vm.IdSociedad = ms.IdSociedad&#xD;&#xA;&#x9;&#x9;&#x9;LEFT JOIN MA_Componente mc ON ms.Id = mc.IdSeccion AND ms.IdSociedad = mc.IdSociedad&#xD;&#xA;&#x9;&#x9;&#x9;LEFT JOIN CatalogoCurso cc ON cc.Id = mc.IdUnidadCurricular AND mc.IdSociedad = cc.IdSociedad&#xD;&#xA;&#x9;)&#xD;&#xA;&#x9;INSERT INTO #UC_ReporteGlobal &#xD;&#xA;&#x9;&#x9;(IdReporteGlobal,IdUC,Fecha,Nota, Estado)&#xD;&#xA;&#x9;SELECT DISTINCT rgm.id as idReporteGlobal,uc.idUc,&#x9;&#x9;&#xD;&#xA;&#x9;ISNULL((CASE WHEN CAST(uc.idUc AS VARCHAR)  IN (SELECT MA_Componente.IdUnidadCurricular &#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9; FROM MA_Version  &#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;INNER JOIN MA_Seccion ON MA_Seccion.IdVersion = MA_Version.Id AND MA_Version.IdMalla =  rgm.[Id Malla] &#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;INNER JOIN MA_Componente ON MA_Componente.IdSeccion = MA_Seccion.Id) &#xD;&#xA;&#x9;&#x9;THEN 'N/D' ELSE '-' END ),'-') AS Fecha,&#xD;&#xA;&#x9;ISNULL((CASE WHEN CAST(uc.idUc AS VARCHAR)  IN (SELECT MA_Componente.IdUnidadCurricular &#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9; FROM MA_Version  &#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;INNER JOIN MA_Seccion ON MA_Seccion.IdVersion = MA_Version.Id AND MA_Version.IdMalla =  rgm.[Id Malla] &#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;INNER JOIN MA_Componente ON MA_Componente.IdSeccion = MA_Seccion.Id) &#xD;&#xA;&#x9;&#x9;THEN 'N/D' ELSE '-' END ),'-') AS Nota,&#xD;&#xA;&#x9;ISNULL((CASE WHEN CAST(uc.idUc AS VARCHAR)  IN (SELECT MA_Componente.IdUnidadCurricular &#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9; FROM MA_Version  &#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;INNER JOIN MA_Seccion ON MA_Seccion.IdVersion = MA_Version.Id AND MA_Version.IdMalla =  rgm.[Id Malla] &#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;INNER JOIN MA_Componente ON MA_Componente.IdSeccion = MA_Seccion.Id) &#xD;&#xA;&#x9;&#x9;THEN 'N/D' ELSE '-' END ),'-') AS Estado&#xD;&#xA;&#x9;FROM #ReporteGlobalMallas rgm&#xD;&#xA;&#x9;&#x9;CROSS JOIN cteUninadesCurriculares uc&#xD;&#xA;&#x9;&#x9;INNER JOIN MA_Nomina n on n.id = rgm.IdNominaMalla AND rgm.Persona_Id = n.idPersona&#xD;&#xA;&#x9;&#x9;LEFT JOIN MA_DetalleNOmina dn on dn.IdNomina = n.id&#xD;&#xA;&#x9;WHERE uc.idUc IS NOT NULL&#xD;&#xA;&#x9;ORDER BY rgm.id,uc.idUc&#xD;&#xA;&#xD;&#xA;&#x9;UPDATE #UC_ReporteGlobal&#xD;&#xA;&#x9;SET Fecha = ISNULL((CASE WHEN dn.IdSituacionFinal IN (1,2,3) THEN &#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;(CASE WHEN dn.FechaActualizacion IS NULL THEN 'N/D' ELSE CONVERT(varchar, dn.FechaActualizacion , 105) END) END),'-'),&#xD;&#xA;&#x9;Nota = (CASE &#xD;&#xA;&#x9;&#x9;&#x9;WHEN dn.IdSituacionFinal IN (1) THEN  &#xD;&#xA;&#x9;&#x9;&#x9;&#x9;(CASE WHEN (dn.PorcentajeEvaluacion) IS NOT NULL &#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;  THEN (CASE WHEN (dn.PorcentajeEvaluacion &lt;> '-1') THEN CAST(dn.PorcentajeEvaluacion AS VARCHAR) + '%' ELSE 'N/A' END) ELSE 'N/D' END)&#xD;&#xA;&#x9;WHEN dn.IdSituacionFinal IN (2,3) THEN &#xD;&#xA;&#x9;&#x9;(CASE WHEN (dn.PorcentajeEvaluacion) IS NOT NULL &#xD;&#xA;&#x9;&#x9;&#x9;  THEN (CASE WHEN (dn.PorcentajeEvaluacion &lt;> '-1') THEN CAST(dn.PorcentajeEvaluacion AS VARCHAR) + '%' ELSE 'N/A' END) ELSE 'N/D' END)&#xD;&#xA;&#x9;&#x9;ELSE 'N/D'  END),&#xD;&#xA;&#x9;Estado = CASE WHEN dn.IdSituacionFinal IN (1) THEN 'Aprobado' WHEN dn.IdSituacionFinal IN (2,3) THEN 'Reprobado' ELSE 'N/D'  END&#xD;&#xA;&#x9;FROM #ReporteGlobalMallas tm&#xD;&#xA;&#x9;&#x9;INNER JOIN MA_Nomina n ON n.id = tm.IdNominaMalla AND tm.Persona_Id = n.idPersona&#xD;&#xA;&#x9;&#x9;INNER JOIN MA_DetalleNOmina dn ON dn.IdNomina = n.id&#xD;&#xA;&#x9;WHERE tm.id = #UC_ReporteGlobal.IdReporteGlobal AND dn.IdUc = #UC_ReporteGlobal.IdUC&#xD;&#xA;&#xD;&#xA;SELECT&#x9;DISTINCT&#xD;&#xA;&#x9;&#x9;#ReporteGlobalMallas.[Id Malla], &#xD;&#xA;&#x9;&#x9;#ReporteGlobalMallas.Malla,&#xD;&#xA;&#x9;&#x9;#ReporteGlobalMallas.[Fecha Creación], &#xD;&#xA;&#x9;&#x9;#ReporteGlobalMallas.Estado, &#xD;&#xA;        #ReporteGlobalMallas.Escuela, &#xD;&#xA;&#x9;&#x9;#ReporteGlobalMallas.[Versión Malla],&#xD;&#xA;&#x9;&#x9;SUBSTRING(#ReporteGlobalMallas.[Vigencia Versión],1,10) AS [Vigencia Versión Desde],&#xD;&#xA;&#x9;&#x9;SUBSTRING(#ReporteGlobalMallas.[Vigencia Versión],14,LEN(#ReporteGlobalMallas.[Vigencia Versión])) AS [Vigencia Versión Hasta], &#xD;&#xA;        #ReporteGlobalMallas.[Estado Versión], &#xD;&#xA;&#x9;&#x9;#ReporteGlobalMallas.[Número Itinerario], &#xD;&#xA;&#x9;&#x9;#ReporteGlobalMallas.[Nombre Itinerario],&#xD;&#xA;        SUBSTRING(#ReporteGlobalMallas.[Vigencia Itinerario],1,10) AS [Itinerario Vigencia Desde],&#xD;&#xA;&#x9;&#x9;SUBSTRING(#ReporteGlobalMallas.[Vigencia Itinerario],14,LEN(#ReporteGlobalMallas.[Vigencia Itinerario])) AS [Itinerario Vigencia Hasta],&#xD;&#xA;&#x9;&#x9;#ReporteGlobalMallas.[Estado Itinerario],&#xD;&#xA;&#x9;&#x9;#ReporteGlobalMallas.RUT, &#xD;&#xA;        #ReporteGlobalMallas.[Nombre Colaborador], &#xD;&#xA;&#x9;&#x9;MA_Nomina.FechaInscripcion AS [Fecha de Matricula],&#xD;&#xA;&#x9;&#x9;#ReporteGlobalMallas.EstadoColaborador, &#xD;&#xA;        #ReporteGlobalMallas.Email, &#xD;&#xA;&#x9;&#x9;#ReporteGlobalMallas.[Sociedad Contratante], &#xD;&#xA;&#x9;&#x9;#ReporteGlobalMallas.[Unidad Organizacional], &#xD;&#xA;        #ReporteGlobalMallas.[Unidad de Negocio], &#xD;&#xA;&#x9;&#x9;ISNULL(#ReporteGlobalMallas.Área,'') AS Area,&#xD;&#xA;&#x9;&#x9;#ReporteGlobalMallas.Local AS [Nombre Local], &#xD;&#xA;        #ReporteGlobalMallas.Local_Codigo AS [Código Local], &#xD;&#xA;&#x9;&#x9;#ReporteGlobalMallas.Cargo, &#xD;&#xA;&#x9;&#x9;#ReporteGlobalMallas.[Familia Cargo], &#xD;&#xA;        ISNULL(#ReporteGlobalMallas.[Rut Jefe] ,'') AS [Rut Jefe], &#xD;&#xA;&#x9;&#x9;ISNULL(#ReporteGlobalMallas.[Nombre Jefe],'') AS [Nombre Jefe], &#xD;&#xA;&#x9;&#x9;ISNULL(#ReporteGlobalMallas.[Email Jefe],'') AS [Email Jefe], &#xD;&#xA;        #ReporteGlobalMallas.Avance, &#xD;&#xA;&#x9;&#x9;CASE WHEN #ReporteGlobalMallas.Avance &lt; 100 THEN 'No Completada' ELSE 'Completada' END AS [Estado de Avance],&#xD;&#xA;&#x9;&#x9;#ReporteGlobalMallas.Cantidad, &#xD;&#xA;&#x9;&#x9;#ReporteGlobalMallas.Aprobadas, &#xD;&#xA;        #ReporteGlobalMallas.Reprobadas, &#xD;&#xA;&#x9;&#x9;#ReporteGlobalMallas.Pendientes &#xD;&#xA;FROM&#x9;&#xD;&#xA;        #UC_ReporteGlobal&#xD;&#xA;&#x9;&#x9;INNER JOIN #ReporteGlobalMallas ON #UC_ReporteGlobal.IdReporteGlobal = #ReporteGlobalMallas.Id &#xD;&#xA;&#x9;&#x9;LEFT OUTER JOIN MA_Nomina ON #ReporteGlobalMallas.IdNominaMalla = MA_Nomina.Id&#xD;&#xA;WHERE (#UC_ReporteGlobal.Fecha &lt;> '-') &#xD;&#xA;&#x9;AND MA_Nomina.FechaInscripcion = (SELECT MAX(n.FechaInscripcion) FROM  MA_Nomina n&#xD;&#xA;&#x9;&#x9;INNER JOIN #ReporteGlobalMallas gm ON gm.IdNominaMalla = n.id AND n.IdPersona = gm.Persona_Id&#xD;&#xA;&#x9;&#x9;WHERE n.IdPersona = MA_Nomina.IdPersona)&#xD;&#xA;ORDER BY #ReporteGlobalMallas.[Id Malla];&#xD;&#xA;&#xD;&#xA;DROP TABLE #ReporteGlobalMallas&#xD;&#xA;DROP TABLE #UC_ReporteGlobal&#xD;&#xA;&quot;"/>
